  {% when 'plus' %}
    <!--
      <svg
        xmlns="http://www.w3.org/2000/svg"
        width="9"
        height="9"
        viewBox="0 0 9 9"
        fill="none"
        class="{{ class }}"
      >
        <line y1="4.5" x2="9" y2="4.5" stroke="black"/>
        <line x1="4.5" y1="9" x2="4.5" y2="2.18557e-08" stroke="black"/>
      </svg>
    -->
    <svg class="{{ class }}" xmlns="http://www.w3.org/2000/svg" width="20" height="21" viewBox="0 0 20 21" fill="none">
      <path d="M9.99996 4.66669V16.3334M9.99996 16.3334L15.8333 10.5M9.99996 16.3334L4.16663 10.5" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'minus' %}
    <!--
      <svg
        xmlns="http://www.w3.org/2000/svg"
        width="9"
        height="1"
        viewBox="0 0 9 1"
        fill="none"
        class="{{ class }}"
      >
        <line y1="0.5" x2="9" y2="0.5" stroke="black"/>
      </svg>
    -->
    <svg class="{{ class }}" xmlns="http://www.w3.org/2000/svg" width="20" height="20" viewBox="0 0 20 20" fill="none">
      <path d="M9.99967 15.8333L9.99967 4.16665M9.99967 4.16665L4.16634 9.99998M9.99967 4.16665L15.833 9.99998" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'breadcumb' %}
    <svg width="25px" height="25px" viewBox="0 0 32 32" xmlns="http://www.w3.org/2000/svg" fill="none">
      <path stroke="#535358" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 8h22M5 16h22M5 24h22"/>
    </svg>
  {% when 'close' %}
    <svg
      class="icon icon-close"
      aria-hidden="true"
      focusable="false"
      xmlns="http://www.w3.org/2000/svg"
      width="22"
      height="22"
      viewBox="0 0 22 22"
      fill="none"
    >
      <path d="M21.0003 20.7982L1.20215 1" stroke="white" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"></path>
      <path d="M20.7982 1.20178L1 21" stroke="white" stroke-miterlimit="10" stroke-linecap="round" stroke-linejoin="round"></path>
    </svg>
  {% when 'plus-menu' %}
    <svg class="plus-icon" xmlns="http://www.w3.org/2000/svg" width="24" height="24" viewBox="0 0 24 24" fill="none">
      <path d="M6 9L12 15L18 9" stroke="#070707" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
  {% when 'minus-menu' %}
    <svg
      class="minus-icon-new"
      xmlns="http://www.w3.org/2000/svg"
      width="24"
      height="24"
      viewBox="0 0 24 24"
      fill="none"
    >
      <path d="M6 15L12 9L18 15" stroke="#070707" stroke-width="2" stroke-linecap="round" stroke-linejoin="round"/>
    </svg>
{% endcase %}
